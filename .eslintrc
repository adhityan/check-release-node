{
    "extends": ["airbnb", "prettier"],
    "env": {
        "es6": true,
        "node": true,
        "jest": true
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "semi": true,
                "trailingComma": "all",
                "singleQuote": true,
                "printWidth": 120,
                "tabWidth": 4,
            }
        ]
    },
    "plugins": ["prettier"]
}
